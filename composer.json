{
    "name": "winwin/omniauth",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "webmozart/assert": "^1.2",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "http-interop/http-factory-diactoros": "^1.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "hybridauth/hybridauth": "~3.0",
        "vlucas/phpdotenv": "^3.3",
        "psr/container": "^1.0"
    },
    "suggest": {
        "hybridauth/hybridauth": "To use oauth2 providers implements in Hybridauth 3"
    },
    "autoload": {
        "psr-4": {
            "winwin\\omniauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "winwin\\omniauth\\": "tests/",
            "": "tests/strategies/"
        }
    }
}
